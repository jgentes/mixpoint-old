{
  "extends": "@tsconfig/node12/tsconfig.json",
  "compilerOptions": {
    "lib": ["es5", "es6", "dom", "dom.iterable"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "target": "ES6",
    "outDir": "dist",
    "sourceMap": true
  },
  "strict": false,
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
